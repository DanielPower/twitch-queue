{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"types": ["bun-types"],
		"lib": ["esnext"],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"moduleDetection": "force",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
